<%= flash[:notice] %>
<h1>Editing Book</h1>


<%= form_with model: @book,method: :patch do |f| %>

<h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Boby</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Update book' %>

  <%= link_to 'show', book_path(@book.id)%>
  <%= link_to 'back', books_path(@book.id)%>

<% end %>
